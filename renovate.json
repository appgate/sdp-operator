{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "pip-compile": {
    "fileMatch": ["(^|/)requirements\\.in$"]
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "enabled": false
    },
    {
      "matchManagers": ["pip-compile"],
      "enabled": true
    }
  ]
}
